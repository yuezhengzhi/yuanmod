    exclude_day_zero_contact = scope_group[target_country]
    ## cardinality = 0..1
    released_by_country = scope_group[target_country]	#scope
    ## cardinality = 0..1
    released_from_country = scope_group[target_country]	#scope
    ## cardinality = 0..1
    ignore_initial_colony_error = bool
    ## cardinality = 0..1
